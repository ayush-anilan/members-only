<%- include("./layout.ejs") %>
  <h1>
    <%= title %>
  </h1>
  <p>Welcome to <%= title %>
  </p>
  <% messages.forEach(message=> { %>
    <div>
      <p>
        <%= user ? message.author.username : "Club Member" %>
      </p>
      <p>
        <%= user ? message.date : "" %>
      </p>
      <p>
        <%= message.content %>
      </p>
    </div>
    <% }) %>


      </body>

      </html>